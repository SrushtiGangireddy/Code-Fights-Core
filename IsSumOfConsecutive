int isSumOfConsecutive2(int n) {
    
    int counter=0;
    
    for(int i=1;i<n;i++){
        int number=n;
        int temp=i;
        
        while(number > 0){
            number -= temp++;
        }
        
        if(number == 0){
            counter++;
        }
        
    }
    
    return counter;
    

}
