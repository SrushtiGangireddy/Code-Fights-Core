int leastFactorial(int n) {
    int result=0;
    for(int i=1;i<=100;i++){
       int temp=fact(i);
       if(temp >= n){
           result=temp;
           return temp;
       }
    }
    return result;
}

int fact(int n){
    
    if((n == 1)){
        return 1;
    }
        return n*fact(n-1);

    
}

